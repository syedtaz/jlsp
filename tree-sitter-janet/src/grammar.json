{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "janet",
  "rules": {
    "main": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "value"
      }
    },
    "value": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "ws"
              },
              {
                "type": "SYMBOL",
                "name": "reader_mac"
              }
            ]
          }
        },
        {
          "type": "SYMBOL",
          "name": "raw_value"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "ws"
          }
        }
      ]
    },
    "ws": {
      "type": "PATTERN",
      "value": "[ \t\r\f\n\u0000\u000b]"
    },
    "reader_mac": {
      "type": "PATTERN",
      "value": "[';~,|]"
    },
    "raw_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "keyword"
        },
        {
          "type": "SYMBOL",
          "name": "comment"
        },
        {
          "type": "SYMBOL",
          "name": "symbol"
        }
      ]
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "#"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    },
    "constant": {
      "type": "STRING",
      "value": ""
    },
    "number": {
      "type": "STRING",
      "value": ""
    },
    "string": {
      "type": "STRING",
      "value": ""
    },
    "buffer": {
      "type": "STRING",
      "value": ""
    },
    "long_string": {
      "type": "STRING",
      "value": ""
    },
    "long_buffer": {
      "type": "STRING",
      "value": ""
    },
    "parray": {
      "type": "STRING",
      "value": ""
    },
    "barray": {
      "type": "STRING",
      "value": ""
    },
    "ptuple": {
      "type": "STRING",
      "value": ""
    },
    "btuple": {
      "type": "STRING",
      "value": ""
    },
    "struct": {
      "type": "STRING",
      "value": ""
    },
    "dict": {
      "type": "STRING",
      "value": ""
    },
    "symbol": {
      "type": "SYMBOL",
      "name": "many_symchars"
    },
    "many_symchars": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "PATTERN",
            "value": "[a-zA-Z0-9-ÿ]"
          },
          {
            "type": "PATTERN",
            "value": "[!$%&*+-./:<?=>@^_]"
          }
        ]
      }
    },
    "keyword": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "many_symchars"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [
    [
      "value"
    ],
    [
      "many_symchars"
    ]
  ],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}